<%= @users %>

<ul class='list-group'>
    <% @users.each do |user| %>
        <li class="list-group-item">
          <h1> Here are all of my users. </h1>
          <%= user.name %>
        </li>
        <li class="list-group-item">
          <%= user.tasks.each do |task| %>
          	<% task.is_completed  %>
          <% end %>
        </li>
    <% end %>
